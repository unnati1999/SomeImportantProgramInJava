public class Fabonacci{
    public static void main(String[]args)
    {
        int n1=0,n2=1;
        System.out.println(n1+","+n2+",");
        for(int i=3;i<=10;i++)
        {
            int nextValue=n1+n2;
            System.out.println(nextValue+",");
            n1=n2;
            n2=nextValue;
        }
        System.out.println("\b");
    }
}
