public class fab {
    static int a=0,b=1,c;
    public static void main(String[]args)
    {
    System.out.println(a);
     System.out.println(b);
     fab obj=new fab();
     obj.printFab(10);
     
}
void printFab(int i)
{
    if(i>=1)
    {
        c=a+b;
         System.out.println(c);
         a=b;
         b=c;
         printFab(i-1);
    }
}
}
