public class Array{
    public static void main(String[]args)
    {
        int arr[]={1,2,3,4,5};
        int sum=0;
        int avg=0;
        System.out.println("Before Reverse Array");
        for(int i=0;i<arr.length;i++)
        {
          System.out.print(arr[i] + " ");  
        }
         System.out.println("after Reverse Array");
         for (int i = arr.length-1; i >= 0; i--) 
        {
          System.out.print(arr[i] + " ");  
        }
       
    }
}
